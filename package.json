{
  "name": "vue-vben-admin-tdesign",
  "version": "5.5.9",
  "description": "基于 Vue Vben Admin v5.5.9 独立拆分的 TDesign 版本单页应用（SPA），完全脱离 monorepo 依赖，可独立部署和发布。",
  "private": true,
  "homepage": "https://ducrong.com/vue-vben-admin-tdesign",
  "bugs": "https://github.com/Ducr/vue-vben-admin-tdesign/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ducr/vue-vben-admin-tdesign.git"
  },
  "author": {
    "name": "Ducr",
    "email": "ducrong@126.com",
    "url": "https://github.com/Ducr"
  },
  "type": "module",
  "scripts": {
    "build": "vite build --mode production",
    "build:analyze": "vite build --mode analyze",
    "changeset": "changeset",
    "dev": "vite --mode development",
    "dev:all": "pnpm run dev & pnpm run dev:docs",
    "dev:docs": "pnpm --filter @vben/docs run dev",
    "build:docs": "pnpm --filter @vben/docs run build",
    "predev:docs": "cd internal/node-utils && npx unbuild --stub >nul 2>&1 && cd ../vite-config && npx unbuild --stub >nul 2>&1 && cd ../..",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "dependencies": {
    "@vben/access": "workspace:*",
    "@vben/common-ui": "workspace:*",
    "@vben/constants": "workspace:*",
    "@vben/hooks": "workspace:*",
    "@vben/icons": "workspace:*",
    "@vben/layouts": "workspace:*",
    "@vben/locales": "workspace:*",
    "@vben/plugins": "workspace:*",
    "@vben/preferences": "workspace:*",
    "@vben/request": "workspace:*",
    "@vben/stores": "workspace:*",
    "@vben/styles": "workspace:*",
    "@vben/types": "workspace:*",
    "@vben/utils": "workspace:*",
    "@vueuse/core": "^13.4.0",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "tdesign-vue-next": "^1.17.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "catalog:",
    "@iconify/tailwind": "^1.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jsonwebtoken": "catalog:",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "eslint": "^9.30.0",
    "eslint-plugin-vue": "^10.2.0",
    "@manypkg/get-packages": "catalog:",
    "get-port": "catalog:",
    "globals": "^16.3.0",
    "h3": "catalog:",
    "happy-dom": "^12.10.3",
    "jsonwebtoken": "catalog:",
    "nitropack": "catalog:",
    "postcss": "^8.5.6",
    "postcss-antd-fixes": "^0.2.0",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.2.4",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.2",
    "vite-plugin-html": "catalog:",
    "vitest": "^3.2.4",
    "vue-tsc": "2.2.10"
  },
  "packageManager": "pnpm@10.14.0"
}
